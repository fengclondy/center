<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.htd.basecenter.dao.MemberCompanyInfoDao">
	 <select id="querySellerIdByCode" parameterType="java.lang.Long" resultType="String">
   		select company_code as comCode from member_company_info where member_id=#{sellerId}
   		and buyer_seller_type='2'
   		limit 1
   </select>
</mapper>