<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						   http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="redissonClientUtil" class="cn.htd.marketcenter.common.utils.RedissonClientUtil" init-method="init">
		<property name="redisAddress" value="${redis.host}:${redis.port}"/>
		<property name="connectionPoolSize" value="${redis.maxTotal}"/>
	</bean>
</beans>
