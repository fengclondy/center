<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
    <!--砍价接口-->
	<dubbo:service interface="cn.htd.promotion.api.BuyerBargainAPI" ref="buyerBargainAPI" version="${dubbo.version}" timeout="${dubbo.timeout}" />
	<dubbo:service interface="cn.htd.promotion.api.BuyerBargainRecordAPI" ref="buyerBargainRecordAPI" version="${dubbo.version}" timeout="${dubbo.timeout}" />
	<dubbo:service interface="cn.htd.promotion.api.PromotionBargainInfoAPI" ref="promotionBargainInfoAPI" version="${dubbo.version}" timeout="${dubbo.timeout}" />
	<dubbo:service interface="cn.htd.promotion.api.PromotionSloganAPI" ref="promotionSloganAPI" version="${dubbo.version}" timeout="${dubbo.timeout}" />
	<dubbo:service interface="cn.htd.promotion.api.AwardRecordAPI" ref="awardRecordAPI" version="${dubbo.version}" timeout="${dubbo.timeout}" />
</beans>
